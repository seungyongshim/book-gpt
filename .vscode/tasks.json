{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "vite dev server",
      "type": "shell",
      "command": "npm run dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "pattern": {
          "regexp": ".*"
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": "ready in .*ms"
        }
      }
    }
  ]
}
